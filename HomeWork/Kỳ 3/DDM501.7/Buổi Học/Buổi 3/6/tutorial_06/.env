# ==========================================
# MLflow Multi-User Environment Configuration
# ==========================================
# Copy this file to .env and customize for your user

# USER is automatically set from $USER environment variable
# No need to set it manually

# ==========================================
# PORT CONFIGURATION
# ==========================================
# Customize these ports if you're running multiple instances
# or if default ports are already in use

# PostgreSQL port (default: 5432)
POSTGRES_PORT=5432

# MinIO API port (default: 9000)
MINIO_API_PORT=9000

# MinIO Console port (default: 9001)
MINIO_CONSOLE_PORT=9001

# MLflow UI port (default: 5000)
MLFLOW_PORT=5001

# ==========================================
#           OTHER ENVIRONMENT   
# ==========================================
MLFLOW_TRACKING_URI = "http://localhost:5001"
MLFLOW_S3_ENDPOINT_URL = "http://localhost:9000"
MLFLOW_S3_IGNORE_TLS = true
AWS_ACCESS_KEY_ID = minio
AWS_SECRET_ACCESS_KEY = minio123
MLFLOW_S3_BUCKET = mlflow
# ==========================================
# EXAMPLE CONFIGURATIONS FOR MULTIPLE USERS
# ==========================================

# User 1 (using default ports):
# POSTGRES_PORT=5432
# MINIO_API_PORT=9000
# MINIO_CONSOLE_PORT=9001
# MLFLOW_PORT=5000

# User 2 (using alternative ports):
# POSTGRES_PORT=5433
# MINIO_API_PORT=9010
# MINIO_CONSOLE_PORT=9011
# MLFLOW_PORT=5001

# User 3 (using alternative ports):
# POSTGRES_PORT=5434
# MINIO_API_PORT=9020
# MINIO_CONSOLE_PORT=9021
# MLFLOW_PORT=5002

